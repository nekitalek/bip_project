server {
	listen 80 default_server;
	listen [::]:80 default_server;
	server_name 192.168.6.144;
	return 302 https://$server_name$request_uri;
}
server {
# SSL configuration
	listen 443 ssl http2 default_server;
	listen [::]:443 ssl http2 default_server;
	include snippets/self-signed.conf;
	include snippets/ssl-params.conf;
        root /home/dmitry/bip_project/frontend/src/;
        #index main.html;

        location /back/ {
	        proxy_pass https://192.168.6.144:65000;
        }
	location = /main/ {
		root /home/dmitry/bip_project/frontend/src;
                index main.html;
        }
	location /login {
                root /home/dmitry/bip_project/frontend/src;
                index login.html;
        }
	location /register {
		root /home/dmitry/bip_project/frontend/src;
                index register.html;
        }
	location /settings {
		root /home/dmitry/bip_project/frontend/src;
                index settings.html;
        }	

}